{
  "name": "auggie-credits",
  "displayName": "Auggie Credits",
  "description": "Track your Auggie credits directly in VS Code status bar",
  "version": "1.2.4",
  "publisher": "augieecredit",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/icon.png",
  "keywords": [
    "auggie",
    "credits",
    "tracker",
    "status",
    "augment"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "auggieCredits.setPortalLink",
        "title": "Set Auggie Portal Link",
        "category": "Auggie Credits"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No compilation needed for JavaScript'",
    "watch": "echo 'No watch needed for JavaScript'",
    "pretest": "npm run compile && npm run lint",
    "lint": "echo 'No linting configured'",
    "test": "echo 'No tests configured'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svsairevanth12/augment-credits.git"
  },
  "bugs": {
    "url": "https://github.com/svsairevanth12/augment-credits/issues"
  },
  "homepage": "https://github.com/svsairevanth12/augment-credits#readme",
  "license": "MIT"
}
